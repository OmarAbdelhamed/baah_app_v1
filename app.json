{
  "expo": {
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/Cairo.ttf"]
        }
      ]
    ],
    "name": "baah_app",
    "slug": "baah_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
